{
    "cli": {
        "version": ">= 14.0.2",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "distribution": "internal"
        },
        "production": {
            "autoIncrement": true
        },
        "myprofile": {
            "releaseChannel": "staging",
            "android": {
                "buildType": "apk",
                "gradleCommand": ":app:assembleRelease"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
